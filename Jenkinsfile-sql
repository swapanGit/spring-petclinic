pipeline{
    agent any
    stages{
        
                stage("liquibase db update"){
            steps{
                bat "mvn liquibase:update"
            }

        }
        
                stage("liquibase db output change"){
            steps{
                bat "mvn liquibase:generateChangeLog"
            }

        }
       
        post {
        always {
            cleanWs(cleanWhenNotBuilt: false,
                    deleteDirs: true,
                    disableDeferredWipeout: true,
                    notFailBuild: true)
        }
    }

    }

}
