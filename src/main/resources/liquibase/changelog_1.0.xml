<?xml version="1.0" encoding="UTF-8"?>

<databaseChangeLog
        xmlns="https://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="https://www.w3.org/2001/XMLSchema-instance"
        xmlns:pro="https://www.liquibase.org/xml/ns/pro"
        xsi:schemaLocation="https://www.liquibase.org/xml/ns/dbchangelog https://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.4.xsd
    https://www.liquibase.org/xml/ns/pro https://www.liquibase.org/xml/ns/pro/liquibase-pro-4.4.xsd ">

	<changeSet id="create_student" author="swapan">

		<createTable tableName="student">

			<column name="id" type="int">

				<constraints primaryKey="true" nullable="false" />

			</column>

			<column name="name" type="varchar(50)">

				<constraints nullable="false" />

			</column>

		</createTable>

	</changeSet>



	<changeSet id="create_employee" author="swapan">

		<createTable tableName="employee">

			<column name="id" type="int">

				<constraints primaryKey="true" nullable="false" />

			</column>

			<column name="firstname" type="varchar(50)">

				<constraints nullable="false" />

			</column>

			<column name="lastname" type="varchar(50)">

				<constraints nullable="false" />

			</column>

		</createTable>

	</changeSet>



</databaseChangeLog>